<div style="margin-left:320px ; width:78% ; margin-top:100px">
    
    <table class="table">
        <tbody>
          
            <tr>
                <th>Nom</th>
                <td>{{ point_vente.name }}</td>
            </tr>
            <tr>
                <th>Region</th>
                <td>{{ point_vente.region }}</td>
            </tr>
            <tr>
                <th>Ville</th>
                <td>{{ point_vente.ville }}</td>
            </tr>
            <tr>
                <th>Code Postal</th>
                <td>{{ point_vente.codePostal }}</td>
            </tr>
            <tr>
                <th>Telephone</th>
                <td>{{ point_vente.telephone }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ point_vente.email }}</td>
            </tr>
        </tbody>
    </table>
</div>
<center>
    <a href="{{ path('get_admin_pointvente') }}">back to list</a>

    

    {{ include('admin/pointvente/_delete_form.html.twig') }}
