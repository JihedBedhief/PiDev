{% extends 'base1.html.twig' %}

{% block title %}admin 
{% endblock %}

{% block body %}
	<h1>Client</h1>
	{# <div style="margin-left:320px ; width:78% ; margin-top:100px"> #}
<center>
		<table class="table">
			<tbody>
				<tr>
					<th>Name</th>
					<td>{{ client.name }}</td>
				</tr>
				<tr>
					<th>Telephone</th>
					<td>{{ client.telephone }}</td>
				</tr>
				<tr>
					<th>Email</th>
					<td>{{ client.email }}</td>
				</tr>
				<tr>
					<th>Ville</th>
					<td>{{ client.ville }}</td>
				</tr>
				<tr>
					<th>Code_postal</th>
					<td>{{ client.codePostal }}</td>
				</tr>
				<tr>
					<th>Cin</th>
					<td>{{ client.cin }}</td>
				</tr>
			</tbody>
		</table>
		</center>
	{# </div> #}
	<center>
		<a class="btn btn-success" href="{{ path('admin_get_client') }}" style="margin-left:180px">back to list</a>
		{# {{ include('admin/client/_delete_form.html.twig') }} #}

	</center>
{% endblock %}
